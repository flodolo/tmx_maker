    <string name="convenience_description">Sblocca comodamente l’app utilizzando la tua impronta digitale</string>

    <string name="escaped_quote">Sblocca comodamente l\'app utilizzando la tua impronta digitale</string>
    <string name="escaped_doublequote">Sblocca comodamente l’app utilizzando la tua \"impronta digitale\"</string>
